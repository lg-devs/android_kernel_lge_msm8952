&pmi8950_vadc {
	chan@10 {
		label = "mpp1_usb_id";
		reg = <0x10>;
		qcom,decimation = <0>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <0>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <3>;
	};
};

&pmi8950_mpps {
	mpp@a000 {
		reg = <0xa000 0x100>;
		qcom,pin-num = <1>;
		status = "okay";
		qcom,vin-sel = <2>;
		qcom,mode = <4>; /* AIN input */
		qcom,invert = <1>; /* Enable MPP */
		qcom,ain-route = <0>; /* AMUX 5 */
		qcom,master-en = <1>;
		qcom,src-sel = <0>; /* Function constant */
	};
};

&pm8950_gpios {
	gpio@c300 { /* GPIO 4 */
		reg = <0xc300 0x100>;
		qcom,pin-num = <0x4>;
		status = "ok";
		qcom,mode = <0x0>;
		qcom,pull = <0x0>;
		qcom,vin-sel = <0x2>;
		qcom,src-sel = <0x0>;
	};

	gpio@c400 { /* GPIO 5 */
		reg = <0xc400 0x100>;
		qcom,pin-num = <0x5>;
		status = "ok";
		qcom,mode = <0x0>;
		qcom,pull = <0x0>;
		qcom,vin-sel = <0x2>;
		qcom,src-sel = <0x0>;
	};
};

&soc{
	qcom,msm-thermal {
		qcom,hotplug-temp = <105>;
		qcom,hotplug-temp-hysteresis = <15>;
		qcom,freq-mitigation-temp = <105>;
		qcom,freq-mitigation-temp-hysteresis = <10>;
		qcom,freq-control-list = <&CPU0 &CPU1 &CPU2 &CPU3>;
		qcom,core-control-list = <&CPU0 &CPU1 &CPU2 &CPU3>;
		qcom,freq-mitigation-control-list = <&CPU0 &CPU1 &CPU2 &CPU3>;
		qcom,freq-mitigation-value = <800000>;
		qcom,limit-temp = <90>;
		qcom,freq-step = <1>;
		qcom,core-limit-temp = <100>;
	};

};


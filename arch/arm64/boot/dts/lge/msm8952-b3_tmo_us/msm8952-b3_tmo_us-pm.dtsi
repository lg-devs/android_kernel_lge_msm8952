	lge,charging_controller {
		compatible = "lge,charging_controller";
	};

	lge,battery_id_check {
		compatible = "lge,batt_id";
		lge,pack_name = "BLT20";
		lge,batt_capacity = "4800";
	};

	lge,dock {
		compatible = "lge,dock";
	};

	lge,uevent {
		compatible = "lge,uevent";
